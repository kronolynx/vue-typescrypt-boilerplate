{
  "name": "vue-typescript-poi",
  "version": "0.1.0",
  "author": "Johann Og <johann@witnet.io>",
  "license": "MIT",
  "scripts": {
    "dev": "poi",
    "build": "poi build",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@vue/test-utils": "^1.0.0-beta.13",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "poi": "^9.5.4",
    "poi-preset-typescript": "^9.0.2",
    "pug": "^2.0.3",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "vue-jest": "^2.4.1"
  },
  "lint-staged": {
    "*.ts": [
      "tslint -p ."
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest"
    },
    "testMatch": [
      "**/?(*.)spec.ts"
    ]
  },
  "dependencies": {
    "stylus": "^0.54.5",
    "vue-class-component": "^6.2.0",
    "vue-router": "^3.0.1"
  }
}
